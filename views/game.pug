html 
    head 
        title game
        script(src="/js/funktion.js")  
        link(rel="stylesheet" type="text/css" href="/css/winningRow.css")
    body 
    #container
        #lotteryTitle UDTRÆKNING D. 
            h= ' '+game.date
        #backButton 
            a.buttonBack(href="/")= 'Startside'

        #winningRow
            h2='VÆLG VINDERTAL'
        #intInput1
            input.inputField(type="number", id= "winner1", min = 1, max = 25)
        #intInput2
            input.inputField(type="number", id= "winner2", min = 1, max = 25)
        #intInput3    
            input.inputField(type="number", id= "winner3", min = 1, max = 25)

        #submitButton
            button.buttonSubmit(onclick='javascript: tilfVT('+ JSON.stringify(game) +')') TILFØJ
        #definiteWinningNumbersText 
            h2='ENDELIGE VINDERTAL'
        #definiteWinningNumbers
            h3.finalWinningNumbers=game.winnerArray
        #winningRows 
            h2= 'VINDERRÆKKE(R)'
        #winningNames 
            h2= 'VINDER(E)'
        #winningRowsUL 
            each row in game.winnerRows 
                h3.winnerRow= row
        #winningNameUL
            each winner in game.winners
                h3.winnerName=winner


